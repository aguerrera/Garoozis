<html>
<head>@Model.Title</head>
<body>
    <div class="content">
        @Model.Content
    </div>
    <div>
        <div>All Posts</div>
        <ul>
        @foreach (var post in Model.Posts) {
         <li><a href="/@post.Url">@post.Title</a></li>   
        }
        </ul>
    </div>
</body>
</html>
